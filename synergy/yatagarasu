section: screens
	yatagarasu:
		halfDuplexCapsLock = false
		halfDuplexNumLock = false
		halfDuplexScrollLock = false
		xtestIsXineramaUnaware = false
		switchCorners = none 
		switchCornerSize = 0
	intelbras:
		halfDuplexCapsLock = false
		halfDuplexNumLock = false
		halfDuplexScrollLock = false
		xtestIsXineramaUnaware = false
		switchCorners = none 
		switchCornerSize = 0
  duo:
		halfDuplexCapsLock = false
		halfDuplexNumLock = false
		halfDuplexScrollLock = false
		xtestIsXineramaUnaware = false
		switchCorners = none 
		switchCornerSize = 0
end

section: aliases
	intelbras:
		lintelbras
end

section: links
	yatagarasu:
		left = intelbras
    down = duo
	intelbras:
		right = yatagarasu
  duo:
    up = yatagarasu
end

section: options
	relativeMouseMoves = false
	screenSaverSync = false
	win32KeepForeground = false
	switchCorners = none 
	switchCornerSize = 0
	keystroke(Super+F10) = switchToScreen(yatagarasu)
  keystroke(Super+F11) = switchToScreen(intelbras)
  keystroke(Super+F12) = switchToScreen(duo)
end

